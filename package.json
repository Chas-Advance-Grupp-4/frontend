{
	"name": "frontend",
	"version": "1.0.0",
	"description": "Frontend monorepo for Chas Advance Grupp 4",
	"private": true,
	"workspaces": [
		"customer-frontend",
		"admin-frontend",
		"driver-frontend"
	],
	"scripts": {
		"dev:customer": "npm --workspace customer-frontend run dev",
		"dev:admin": "npm --workspace admin-frontend run dev",
		"dev:driver": "npm --workspace driver-frontend run dev",
		"lint": "npm --workspace common run lint && npm --workspace customer-frontend run lint && npm --workspace admin-frontend run lint && npm --workspace driver-frontend run lint",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"build": "npm --workspace customer-frontend run build && npm --workspace admin-frontend run build && npm --workspace driver-frontend run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Chas-Advance-Grupp-4/frontend.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"type": "module",
	"bugs": {
		"url": "https://github.com/Chas-Advance-Grupp-4/frontend/issues"
	},
	"homepage": "https://github.com/Chas-Advance-Grupp-4/frontend#readme",
	"dependencies": {
		"@heroicons/react": "^2.2.0",
		"@tailwindcss/vite": "^4.1.13",
		"jwt-decode": "^4.0.0",
		"lucide-react": "^0.544.0",
		"tailwindcss": "^4.1.13"
	},
	"devDependencies": {
		"@testing-library/dom": "^10.4.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@types/jest": "^30.0.0",
		"@types/mocha": "^10.0.10",
		"axios": "^1.12.2",
		"jsdom": "^27.0.0",
		"vitest": "^3.2.4"
	}
}
